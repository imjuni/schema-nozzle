{
  "name": "schema-nozzle",
  "version": "0.24.0",
  "description": "json-schema generate from typescript interface and type alias, class, enum",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "clean": "rimraf dist artifact",
    "build": "pnpm run clean && tsc --incremental --project tsconfig.json --tsBuildInfoFile .tsbuildinfo",
    "dev": "ts-node --project ./tsconfig.json src/cli.ts",
    "debug": "node --inspect-brk -r ts-node/register src/cli.ts",
    "ctix": "ctix build --config .configs/.ctirc",
    "ctix-rm": "ctix remove --config ./.configs/.ctirc --force-yes --remove-backup",
    "dts": "tsc -p tsconfig.dts.json",
    "dts-alias": "tsc-alias -p tsconfig.dts.json",
    "dts-gen": "rollup --config ./.configs/rollup.config.mjs",
    "dts-pack": "run-s dts dts-alias dts-gen",
    "dts-rm": "rimraf dist/types/origin",
    "esbuild-lib-cjs": "FORMAT=cjs node .configs/esbuild.mjs",
    "esbuild-lib-esm": "FORMAT=esm node .configs/esbuild.mjs",
    "bundle-lib-pack": "run-p esbuild-lib-cjs esbuild-lib-esm dts-pack",
    "bundle-lib": "run-s ctix bundle-lib-pack ctix-rm",
    "bundle-cli": "FORMAT=cjs node .configs/esbuild.cli.mjs",
    "bundle": "run-s clean bundle-cli bundle-lib",
    "prettier": "prettier --write src/**/*.ts",
    "lint": "eslint --cache .",
    "lint-staged": "lint-staged",
    "test": "jest --coverage",
    "dd": "deps-diff md -d prod -d dev --title-list-type '*' --deps-list-type '*' --content-list-type '*'",
    "lnb": "run-p lint build",
    "pub": "pnpm run bundle && NODE_ENV=production RELEASE_MODE=true pnpm publish --registry http://localhost:8901 --force",
    "pub:prod": "pnpm run bundle && NODE_ENV=production RELEASE_MODE=true pnpm publish",
    "unpub": "pnpm unpublish $npm_package_name@$npm_package_version --registry http://localhost:8901 --force",
    "prepublishOnly": "node prepublish.cjs",
    "prepare": "husky install"
  },
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "default": "./dist/cjs/index.cjs",
        "types": "./dist/types/index.d.ts"
      },
      "import": {
        "default": "./dist/esm/index.mjs",
        "types": "./dist/types/index.d.ts"
      }
    }
  },
  "bin": {
    "nozzle": "dist/cjs/cli.cjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imjuni/schema-nozzle.git"
  },
  "keywords": [
    "schema",
    "json-schema",
    "jsonschema",
    "cli",
    "typescript",
    "ts",
    "json"
  ],
  "author": "ByungJoon Lee",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/imjuni/schema-nozzle/issues"
  },
  "homepage": "https://github.com/imjuni/schema-nozzle#readme",
  "dependencies": {
    "@maeum/cli-logo": "^1.0.0",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "cli-progress": "^3.12.0",
    "dayjs": "^1.11.10",
    "deepmerge": "^4.3.1",
    "fast-copy": "^2.1.7",
    "fast-glob": "^3.3.1",
    "fast-safe-stringify": "^2.1.1",
    "figlet": "^1.6.0",
    "find-up": "^5.0.0",
    "fuse.js": "^6.6.2",
    "ignore": "^5.2.4",
    "inquirer": "^8.2.5",
    "inquirer-autocomplete-prompt": "^2.0.0",
    "inquirer-ts-checkbox-plus-prompt": "^1.0.1",
    "is-plain-object": "^5.0.0",
    "jsonc-parser": "^3.2.0",
    "mathjs": "^11.11.2",
    "minimist": "^1.2.8",
    "my-easy-fp": "^0.21.0",
    "my-node-fp": "^0.9.0",
    "my-only-either": "^1.3.0",
    "object-traversal": "^1.0.1",
    "ora": "^5.4.1",
    "pino": "^8.16.0",
    "pino-pretty": "^10.2.3",
    "prettier": "^3.0.3",
    "rxjs": "^7.8.1",
    "set-interval-async": "^3.0.3",
    "ts-json-schema-generator": "^1.3.0",
    "ts-morph": "^20.0.0",
    "ts-morph-short": "^0.4.0",
    "tslib": "^2.6.2",
    "type-fest": "^4.4.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@slack/web-api": "^6.9.0",
    "@tsconfig/node18": "^18.2.2",
    "@types/cli-progress": "^3.11.3",
    "@types/figlet": "^1.5.6",
    "@types/inquirer": "^9.0.4",
    "@types/inquirer-autocomplete-prompt": "^3.0.1",
    "@types/jest": "^29.5.4",
    "@types/minimist": "^1.2.3",
    "@types/node": "^16.11.7",
    "@types/source-map-support": "^0.5.8",
    "@types/yargs": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "ajv": "^8.12.0",
    "concurrently": "^8.2.1",
    "cross-env": "^7.0.3",
    "ctix": "^2.3.0",
    "deps-diff": "^1.0.2",
    "esbuild": "^0.19.4",
    "eslint": "^8.51.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsdoc": "^46.8.2",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^5.0.1",
    "execa": "^5.1.1",
    "husky": "^8.0.3",
    "jest": "^29.6.4",
    "lint-staged": "^14.0.1",
    "npm-run-all": "^4.1.5",
    "prettier-eslint": "^15.0.1",
    "prettier-plugin-organize-imports": "^3.2.3",
    "read-pkg": "^5.2.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.0.2",
    "rollup-plugin-dts": "^6.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^3.13.0",
    "typescript": "^5.2.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "cross-env NODE_ENV=production eslint --cache"
    ],
    "*.{js,jsx,ts,tsx},*.json,.{eslintrc.cjs,prettierrc}": [
      "cross-env NODE_ENV=production prettier --ignore-path .eslintignore --parser json --write"
    ]
  },
  "peerDependencies": {
    "ajv": "^8.12.0"
  }
}
