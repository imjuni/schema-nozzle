{
  "name": "schema-nozzle",
  "version": "0.25.0",
  "description": "json-schema generate from typescript interface and type alias, class, enum",
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@9.0.6",
  "scripts": {
    "clean": "rimraf dist artifact",
    "build": "pnpm run clean && tsc --incremental --project tsconfig.json --tsBuildInfoFile .tsbuildinfo",
    "dev": "ts-node --project ./tsconfig.json src/cli.ts",
    "debug": "node --inspect-brk -r ts-node/register src/cli.ts",
    "ctix": "ctix build --config .configs/.ctirc",
    "ctix-rm": "ctix remove --config ./.configs/.ctirc --force-yes --remove-backup",
    "dts": "tsc -p tsconfig.dts.json",
    "dts-alias": "tsc-alias -p tsconfig.dts.json",
    "dts-gen": "rollup --config ./.configs/rollup.config.mjs",
    "dts-pack": "run-s dts dts-alias dts-gen",
    "dts-rm": "rimraf dist/types/origin",
    "esbuild-lib-cjs": "FORMAT=cjs node .configs/esbuild.mjs",
    "esbuild-lib-esm": "FORMAT=esm node .configs/esbuild.mjs",
    "bundle-lib-pack": "run-p esbuild-lib-cjs esbuild-lib-esm dts-pack",
    "bundle-lib": "run-s ctix bundle-lib-pack ctix-rm",
    "bundle-cli": "FORMAT=cjs node .configs/esbuild.cli.mjs",
    "bundle": "run-s clean bundle-cli bundle-lib",
    "prettier": "prettier --write src/**/*.ts",
    "lint": "eslint --cache .",
    "lint-staged": "lint-staged",
    "test": "vitest --run --coverage",
    "dd": "deps-diff md -d prod -d dev --title-list-type '*' --deps-list-type '*' --content-list-type '*'",
    "lnb": "run-p lint build",
    "prepub": "run-s unpub bundle",
    "pub": "NODE_ENV=production RELEASE_MODE=true pnpm publish --registry http://localhost:8901 --force",
    "pub:prod": "pnpm run bundle && NODE_ENV=production RELEASE_MODE=true pnpm publish",
    "unpub": "pnpm unpublish $npm_package_name@$npm_package_version --registry http://localhost:8901 --force",
    "prepublishOnly": "node prepublish.cjs",
    "prepare": "husky install"
  },
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "default": "./dist/cjs/index.cjs",
        "types": "./dist/types/index.d.ts"
      },
      "import": {
        "default": "./dist/esm/index.mjs",
        "types": "./dist/types/index.d.ts"
      }
    }
  },
  "bin": {
    "nozzle": "dist/cjs/cli.cjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imjuni/schema-nozzle.git"
  },
  "keywords": ["schema", "json-schema", "jsonschema", "cli", "typescript", "ts", "json"],
  "author": "ByungJoon Lee",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/imjuni/schema-nozzle/issues"
  },
  "homepage": "https://github.com/imjuni/schema-nozzle#readme",
  "dependencies": {
    "@maeum/cli-logo": "^1.0.0",
    "alasql": "^4.3.2",
    "awilix": "^10.0.2",
    "chalk": "^4.1.2",
    "cli-progress": "^3.12.0",
    "comment-parser": "^1.4.1",
    "consola": "^3.2.3",
    "deepmerge": "^4.3.1",
    "dot-prop": "5.3.0",
    "fast-copy": "^2.1.7",
    "fast-safe-stringify": "^2.1.1",
    "figlet": "^1.7.0",
    "find-up": "^5.0.0",
    "fuse.js": "^7.0.0",
    "glob": "^10.3.10",
    "inquirer": "^8.2.5",
    "inquirer-autocomplete-prompt": "^2.0.0",
    "inquirer-ts-checkbox-plus-prompt": "^1.0.1",
    "is-plain-object": "^5.0.0",
    "jsonc-parser": "^3.2.1",
    "lokijs": "^1.5.12",
    "mathjs": "^12.4.1",
    "minimist": "^1.2.8",
    "my-easy-fp": "^0.22.0",
    "my-node-fp": "^0.10.3",
    "my-only-either": "^1.3.0",
    "object-traversal": "^1.0.1",
    "ora": "^5.4.1",
    "pathe": "^1.1.2",
    "prettier": "^3.2.5",
    "ts-json-schema-generator": "2.0.1",
    "ts-morph": "^22.0.0",
    "ts-morph-short": "^0.9.1",
    "ts-pattern": "^5.1.1",
    "tslib": "^2.6.2",
    "type-fest": "^4.13.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@slack/web-api": "^7.0.2",
    "@tsconfig/node18": "^18.2.2",
    "@types/cli-progress": "^3.11.5",
    "@types/figlet": "^1.5.8",
    "@types/inquirer": "^9.0.7",
    "@types/inquirer-autocomplete-prompt": "^3.0.3",
    "@types/lokijs": "^1.5.14",
    "@types/minimist": "^1.2.5",
    "@types/node": "18.15.3",
    "@types/source-map-support": "^0.5.10",
    "@types/yargs": "^17.0.32",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "@vitest/coverage-v8": "^1.5.0",
    "ajv": "^8.12.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "ctix": "^2.4.1",
    "deps-diff": "^1.0.2",
    "esbuild": "^0.20.2",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsdoc": "^48.2.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^5.1.3",
    "execa": "^5.1.1",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "npm-run-all": "^4.1.5",
    "prettier-eslint": "^16.3.0",
    "prettier-plugin-organize-imports": "^3.2.4",
    "read-pkg": "^5.2.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.13.0",
    "rollup-plugin-dts": "^6.1.0",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.5.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": ["cross-env NODE_ENV=production eslint --cache"],
    "*.{js,jsx,ts,tsx},*.json,.{eslintrc.cjs,prettierrc}": [
      "cross-env NODE_ENV=production prettier --ignore-path .eslintignore --parser json --write"
    ]
  },
  "peerDependencies": {
    "ajv": "^8.12.0"
  }
}
