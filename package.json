{
  "name": "schema-nozzle",
  "version": "0.24.0",
  "description": "json-schema generate from typescript interface and type alias, class, enum",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "clean": "rimraf dist artifact",
    "clean:dts": "rimraf dist/cjs/src dist/esm/src dist/src",
    "build": "tsc --incremental --project tsconfig.json --tsBuildInfoFile .tsbuildinfo",
    "dev": "ts-node --project ./tsconfig.json src/cli.ts",
    "debug": "node --inspect-brk -r ts-node/register src/cli.ts",
    "ctix:single": "ctix single -p ./tsconfig.prod.json --config ./.configs/.ctirc",
    "ctix:remove": "ctix remove -p ./tsconfig.prod.json --config ./.configs/.ctirc",
    "lint": "eslint --cache .",
    "lint-staged": "lint-staged",
    "prerollup:dev": "run-s clean lint ctix:single",
    "rollup:dev": "rollup --config ./.configs/rollup.config.dev.ts --configPlugin typescript",
    "postrollup:dev": "run-p ctix:remove clean:dts",
    "prerollup:prod": "run-s clean lint ctix:single",
    "rollup:prod": "rollup --config ./.configs/rollup.config.prod.ts --configPlugin typescript",
    "postrollup:prod": "run-p ctix:remove clean:dts",
    "test": "node --trace-warnings --no-compilation-cache node_modules/.bin/jest --coverage",
    "test:swc": "SWC=true node --trace-warnings --no-compilation-cache node_modules/.bin/jest --coverage",
    "dd": "deps-diff md -d prod -d dev --title-list-type '*' --deps-list-type '*' --content-list-type '*'",
    "lnb": "run-p lint build",
    "prepublishOnly": "node prepublish.cjs",
    "prepub": "npm run rollup:prod",
    "pub": "NODE_ENV=production RELEASE_MODE=true npm publish --registry http://localhost:8901 --force",
    "prepub:prod": "npm run rollup:prod",
    "pub:prod": "NODE_ENV=production RELEASE_MODE=true npm publish",
    "unpub": "unpublish schema-nozzle@latest --registry http://localhost:8901 --force",
    "prepare": "husky install"
  },
  "main": "dist/cli.js",
  "types": "dist/cli.d.ts",
  "bin": {
    "nozzle": "dist/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imjuni/schema-nozzle.git"
  },
  "keywords": [
    "schema",
    "json-schema",
    "jsonschema",
    "cli",
    "typescript",
    "ts",
    "json"
  ],
  "author": "ByungJoon Lee",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/imjuni/schema-nozzle/issues"
  },
  "homepage": "https://github.com/imjuni/schema-nozzle#readme",
  "dependencies": {
    "@maeum/cli-logo": "^0.2.0",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "cli-progress": "^3.12.0",
    "dayjs": "^1.11.9",
    "deepmerge": "^4.3.1",
    "fast-copy": "^2.1.7",
    "fast-glob": "^3.3.1",
    "fast-safe-stringify": "^2.1.1",
    "figlet": "^1.6.0",
    "find-up": "^5.0.0",
    "fuse.js": "^6.6.2",
    "ignore": "^5.2.4",
    "inquirer": "^8.2.5",
    "inquirer-autocomplete-prompt": "^2.0.0",
    "inquirer-ts-checkbox-plus-prompt": "^1.0.1",
    "is-plain-object": "^5.0.0",
    "jsonc-parser": "^3.2.0",
    "mathjs": "^11.9.1",
    "minimist": "^1.2.8",
    "my-easy-fp": "^0.21.0",
    "my-node-fp": "^0.8.1",
    "my-only-either": "^1.3.0",
    "object-traversal": "^1.0.1",
    "ora": "^5.4.1",
    "pino": "^8.14.1",
    "pino-pretty": "^10.2.0",
    "prettier": "^3.0.0",
    "rxjs": "^7.8.1",
    "set-interval-async": "^3.0.3",
    "ts-json-schema-generator": "^1.2.0",
    "ts-morph": "^19.0.0",
    "tslib": "^2.6.1",
    "type-fest": "^4.0.0",
    "typescript": "^5.1.6",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.1.2",
    "@slack/web-api": "^6.8.1",
    "@swc/core": "^1.3.71",
    "@swc/helpers": "^0.5.1",
    "@swc/jest": "^0.2.27",
    "@tsconfig/node16": "^1.0.3",
    "@types/cli-progress": "^3.11.0",
    "@types/figlet": "^1.5.6",
    "@types/inquirer": "^9.0.3",
    "@types/inquirer-autocomplete-prompt": "^3.0.0",
    "@types/jest": "^29.5.3",
    "@types/minimist": "^1.2.2",
    "@types/node": "^16.11.7",
    "@types/source-map-support": "^0.5.6",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3",
    "ctix": "^1.8.2",
    "deps-diff": "^1.0.2",
    "eslint": "^8.45.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^46.4.4",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^5.0.0",
    "execa": "^5.1.1",
    "husky": "^8.0.3",
    "jest": "^29.6.1",
    "jest_workaround": "^0.79.19",
    "just-task": "^1.7.2",
    "lint-staged": "^13.2.3",
    "npm-run-all": "^4.1.5",
    "prettier-eslint": "^15.0.1",
    "prettier-plugin-organize-imports": "^3.2.3",
    "read-pkg": "^5.2.0",
    "rimraf": "^5.0.1",
    "rollup": "^3.26.3",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-swc3": "^0.9.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "cross-env NODE_ENV=production eslint --cache"
    ],
    "*.{js,jsx,ts,tsx},*.json,.{eslintrc.cjs,prettierrc}": [
      "cross-env NODE_ENV=production prettier --ignore-path .eslintignore --parser json --write"
    ]
  }
}
