{
  "name": "schema-nozzle",
  "version": "0.16.0",
  "description": "generate json-schema from typescript source code and management it",
  "scripts": {
    "clean": "just --config ./.configs/just.config.ts clean",
    "build": "just --config ./.configs/just.config.ts build",
    "dev": "ts-node --project ./tsconfig.json src/cli.ts",
    "debug": "node --inspect-brk -r ts-node/register src/cli.ts",
    "lint": "just --config ./.configs/just.config.ts lint",
    "rollup:dev": "just --config ./.configs/just.config.ts rollup:dev",
    "rollup:prod": "just --config ./.configs/just.config.ts rollup:prod",
    "test": "jest --coverage",
    "dd": "deps-diff md -d prod -d dev --title-list-type '*' --deps-list-type '*' --content-list-type '*'",
    "prepublishOnly": "node prepublish.cjs",
    "pub": "just --config ./.configs/just.config.ts pub",
    "pub:prod": "just --config ./.configs/just.config.ts pub:prod",
    "unpub": "just --config ./.configs/just.config.ts unpub"
  },
  "main": "dist/cli.js",
  "types": "dist/cli.d.ts",
  "bin": {
    "nozzle": "dist/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imjuni/create-ts-json-schema.git"
  },
  "keywords": [],
  "author": "ByungJoon Lee",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/imjuni/create-ts-json-schema/issues"
  },
  "homepage": "https://github.com/imjuni/create-ts-json-schema#readme",
  "dependencies": {
    "chalk": "^4.1.2",
    "cli-progress": "^3.11.2",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.7",
    "fast-copy": "^2.1.4",
    "fast-glob": "^3.2.12",
    "fast-safe-stringify": "^2.1.1",
    "find-up": "^5.0.0",
    "fuse.js": "^6.6.2",
    "ignore": "^5.2.4",
    "inquirer": "^8.2.5",
    "inquirer-autocomplete-prompt": "^2.0.0",
    "inquirer-ts-checkbox-plus-prompt": "^1.0.1",
    "is-relative": "^1.0.0",
    "jsonc-parser": "^3.2.0",
    "mathjs": "^11.5.0",
    "minimist": "^1.2.7",
    "my-easy-fp": "^0.19.3",
    "my-node-fp": "^0.8.1",
    "my-only-either": "^1.3.0",
    "object-traversal": "^1.0.1",
    "ora": "^5.4.1",
    "pino": "^8.8.0",
    "pino-pretty": "^9.1.1",
    "prettier": "^2.8.3",
    "ts-json-schema-generator": "^1.2.0",
    "ts-morph": "^17.0.1",
    "tsimportlib": "0.0.3",
    "tslib": "^2.5.0",
    "type-fest": "^3.5.3",
    "url-join": "^5.0.0",
    "url-naong": "^1.0.0",
    "yargs": "^17.6.2"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.4.0",
    "@slack/web-api": "^6.8.0",
    "@tsconfig/node14": "^1.0.3",
    "@types/cli-progress": "^3.11.0",
    "@types/crypto-js": "^4.1.1",
    "@types/inquirer": "^9.0.2",
    "@types/inquirer-autocomplete-prompt": "^3.0.0",
    "@types/is-relative": "^1.0.0",
    "@types/jest": "^29.1.2",
    "@types/minimist": "^1.2.2",
    "@types/node": "^18.11.0",
    "@types/source-map-support": "^0.5.6",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "cross-env": "^7.0.3",
    "deps-diff": "^1.0.2",
    "eslint": "^8.33.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^39.7.4",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.2.1",
    "execa": "^5.1.1",
    "jest": "^29.4.0",
    "just-task": "^1.7.0",
    "prettier-eslint": "^15.0.1",
    "prettier-plugin-organize-imports": "^3.2.2",
    "read-pkg": "^5.2.0",
